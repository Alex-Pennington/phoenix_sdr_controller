/**
 * @file version.h
 * @brief Phoenix project version information
 * 
 * Auto-generated by CMake - DO NOT EDIT
 * Generated from templates/version.h.in by phoenix-build.cmake
 * 
 * Version format: MAJOR.MINOR.PATCH+BUILD.COMMIT[-dirty]
 * Example: 0.1.2+5.abc1234 or 0.1.2+5.abc1234-dirty
 */

#ifndef PHOENIX_VERSION_H
#define PHOENIX_VERSION_H

/* Semantic version components */
#define PHOENIX_VERSION_MAJOR   @PHOENIX_VERSION_MAJOR@
#define PHOENIX_VERSION_MINOR   @PHOENIX_VERSION_MINOR@
#define PHOENIX_VERSION_PATCH   @PHOENIX_VERSION_PATCH@
#define PHOENIX_VERSION_BUILD   @PHOENIX_VERSION_BUILD@

/* Version strings */
#define PHOENIX_VERSION_STRING  "@PHOENIX_VERSION_STRING@"
#define PHOENIX_VERSION_FULL    "@PHOENIX_VERSION_FULL@"

/* Git information */
#define PHOENIX_GIT_COMMIT      "@PHOENIX_GIT_COMMIT@"
#define PHOENIX_GIT_DIRTY       @PHOENIX_GIT_DIRTY@

/* Build timestamp (compiler-generated) */
#define PHOENIX_BUILD_DATE      __DATE__
#define PHOENIX_BUILD_TIME      __TIME__

#include <stdio.h>

/**
 * @brief Print version information to stdout
 * @param tool_name Name of the executable/tool
 * 
 * Example output: "sdr_server v0.1.2+5.abc1234 (built Dec 23 2025 14:30:15)"
 */
static inline void print_version(const char *tool_name) {
    printf("%s v%s (built %s %s)\n",
           tool_name, PHOENIX_VERSION_FULL,
           PHOENIX_BUILD_DATE, PHOENIX_BUILD_TIME);
}

#endif /* PHOENIX_VERSION_H */
